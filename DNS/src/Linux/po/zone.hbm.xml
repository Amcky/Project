<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Linux.po.zone" table="zone">
		<id name="id">
		</id>
		<property name="ip"></property>
		<property name="name"></property>
		<property name="ttl"></property>
		<property name="serial"></property>
		<property name="refresh"></property>
		<property name="retry"></property>
		<property name="expire"></property>
		<property name="minimum"></property>
		<property name="pid"></property>
	</class>	
</hibernate-mapping>

